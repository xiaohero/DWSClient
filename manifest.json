{
  "name": "__MSG_appName__",
  "version": "1.0",
  "manifest_version": 2,
  "default_locale": "en",
  "description": "__MSG_appName__",
  "homepage_url": "http://www.dwsframework.com",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon128_128.jpg",
    "default_title": "DWSClient-V1.0",
    "default_popup": "popup/menu.html"
  },
  "icons": {
    "16": "icon128_128.jpg"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "contextMenus",
    "webNavigation",
    "management",
    "cookies",
    "storage",
    "debugger",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_start",
      "js": [
        "buster.js"
      ],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
  ]
}